{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Conda 调试 main.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "python": "~/miniconda3/envs/RING-env/bin/python"
    }
  ]
}

